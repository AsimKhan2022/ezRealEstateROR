<p style="color: green"><%= notice %></p>

<h1>Tenants</h1>

<%= link_to "New tenant", new_tenant_path, class: "btn btn-primary" %>

<div id="tenants">
  <div class="container mt-4">
    <table class="table table-striped table-bordered">
      <thead class="table-dark">
        <tr>
          <th scope="col">Lead</th>
          <th scope="col">Admin</th>
          <th scope="col">Title</th>
          <th scope="col">Move In Date</th>
          <th scope="col">Move Out Date</th>
        </tr>
      </thead>
      <tbody>
        <% @tenants.each do |tenant| %>
        <tr>
          <td>
            <%= tenant.lead_id %>
          </td>
          <td>
            <%= tenant.admin_id %>
          </td>
          <td>
            <%= tenant.title %>
          </td>
          <td>
            <%= tenant.move_in_date %>
          </td>
          <td>
            <%= tenant.move_out_date %>
          </td>
          <td>
            <%= link_to "Show", tenant_path(tenant), class: "btn btn-primary" %>
          </td>
          <td>
            <%= link_to "Edit", edit_tenant_path(tenant), class: "btn btn-secondary" %>
          </td>
          <td>
            <%= link_to "Delete", tenant_path(tenant), method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-danger" %>
          </td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
